{
  "expo": {
    "name": "SightQuest",
    "slug": "sightquest-client",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera."
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location.",
          "isIosBackgroundLocationEnabled": true,
          "isAndroidBackgroundLocationEnabled": true
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-font",
        {
          "fonts": ["./src/assets/fonts/FormaDJRCyrillicText.ttf"]
        }
      ]
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.shapiniony.sightquest",
      "buildNumber": "9",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app uses your location to give you hints on navigating the city.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app uses your location to give you hints on navigating the city.",
        "NSLocationAlwaysUsageDescription": "This app uses your location to give you hints on navigating the city."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.shapiniony.sightquest"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/c9d40fe9-f090-43e3-80c1-ae5efeaaf7ae"
    },
    "owner": "shampiniony",
    "extra": {
      "eas": {
        "projectId": "c9d40fe9-f090-43e3-80c1-ae5efeaaf7ae"
      }
    }
  }
}
